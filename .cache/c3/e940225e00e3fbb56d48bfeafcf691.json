ransaction=()=>{\n        this.setState({displayTransaction:!this.state.displayTransaction})\n    }\n    get displayTransaction()\n    {\n        const {data} = this.props.block;\n        const stringifiedData=JSON.stringify(data);\n        const dataDisplay=stringifiedData.length >15 ?`${stringifiedData.substring(0,15)}...`:stringifiedData; \n\n        if(this.state.displayTransaction)\n        {\n            return( \n                <div>\n                     {\n                         data.map(transaction=>{\n                            return(\n                                <div key={transaction.id}>\n                                <hr />\n                                <Transaction transaction={transaction} /> \n                            </div>\n                            )\n                         })\n                     }\n                     <Button bsStyle=\"danger\" bsSize=\"small\" onClick={this.toggleTransaction}>Show Less</Button>\n                </div>\n                 )\n        }\n\n        return( \n            <div>\n                 <div>Data:{dataDisplay}</div>\n                 <Button bsStyle=\"danger\" bsSize=\"small\" onClick={this.toggleTransaction}>Show More</Button>\n            </div>\n             )\n\n\n    }\n    render()\n    {\n        const {timestamp , hash} = this.props.block;\n        const hashDisplay=`${hash.substring(0,30)}...`\n        return(\n            <div className='Block'>\n                <div>Hash:{hashDisplay}</div>\n                <div >Timestamp:{new Date(timestamp).toLocaleString()}</div>\n                {this.displayTransaction}\n            </div>\n        )\n    }\n}\n\nexport default Block;"},"lineCount":null}},"error":null,"hash":"f378d8a2b2e3f5ca57fa51f02a7492eb","cacheData":{"env":{}}}